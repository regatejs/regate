<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no" />
    <title>{{ title }} - Regate</title>

    <script src="{{ themeUrl }}js/core/jquery.min.js"></script>
    <script src="{{ themeUrl }}js/core/bootstrap.bundle.min.js"></script>
    <script src="{{ themeUrl }}js/core/simplebar.min.js"></script>
    <script src="{{ themeUrl }}js/core/jquery-scrollLock.min.js"></script>
    <script src="{{ themeUrl }}js/core/jquery.appear.min.js"></script>
    <script src="{{ themeUrl }}js/core/js.cookie.min.js"></script>

    <script src="{{ themeUrl }}regate.umd.js"></script>

    <script>
        window.konsoleClear = container => {
            let $terminal = $('#' + container)
            $terminal.html('')
        }

        window.konsoleGroup = (container, groupName) => {
            let $terminal = $('#' + container)
            $terminal.append('<style>#' + container + ':before { content: "OUTPUT ' + groupName + '"; }</style>');
        }

        window.konsole = (container, type, title, value) => {
            let $terminal = $('#' + container)
            let $pre = $('<pre></pre>')

            if (type.trim() === 'boolean') {
            value = value ? 'true' : 'false'
            }

            $('<code></code>')
            .attr('data-type', 'title')
            .text(title)
            .appendTo($pre)

            $('<code></code>')
            .attr('data-type', type.trim())
            .text(type)
            .appendTo($pre)


            $('<code></code>')
            .attr('data-type', 'value')
            .text(value)
            .appendTo($pre)

            $pre.appendTo($terminal)
        }
        </script>

    <link rel="stylesheet" id="css-main" href="{{ themeUrl }}css/dashmix.min.css">
    <link rel="stylesheet" href="{{ themeUrl }}js/plugins/highlightjs/styles/atom-one-light.css">
</head>
<body>

  <div id="page-container" class="sidebar-o enable-page-overlay side-scroll page-header-fixed page-header-dark main-content-narrow">

      <!-- Sidebar -->
      <!--
          Sidebar Mini Mode - Display Helper classes

          Adding 'smini-hide' class to an element will make it invisible (opacity: 0) when the sidebar is in mini mode
          Adding 'smini-show' class to an element will make it visible (opacity: 1) when the sidebar is in mini mode
              If you would like to disable the transition animation, make sure to also add the 'no-transition' class to your element

          Adding 'smini-hidden' to an element will hide it when the sidebar is in mini mode
          Adding 'smini-visible' to an element will show it (display: inline-block) only when the sidebar is in mini mode
          Adding 'smini-visible-block' to an element will show it (display: block) only when the sidebar is in mini mode
      -->
      <nav id="sidebar" aria-label="Main Navigation">
        <!-- Side Header -->
        <div class="bg-header-dark">
            <div class="content-header bg-white-10">
                <!-- Logo -->
                <a class="link-fx font-w600 font-size-lg text-white" href="/">
                    <span class="smini-visible">
                        <span class="text-white-75">D</span><span class="text-white">x</span>
                    </span>
                    <span class="smini-hidden">
                        <span class="text-white-75">Re</span><span class="text-white">gate</span>
                    </span>
                </a>
                <!-- END Logo -->
                <!-- Options -->
                <div>
                    <!-- END Toggle Sidebar Style -->
                    <!-- Close Sidebar, Visible only on mobile screens -->
                    <!-- Layout API, functionality initialized in Template._uiApiLayout() -->
                    <a class="d-lg-none text-white ml-2" data-toggle="layout" data-action="sidebar_close" href="javascript:void(0)">
                        <i class="fa fa-times-circle"></i>
                    </a>
                    <!-- END Close Sidebar -->
                </div>
                <!-- END Options -->
            </div>
        </div>
        <!-- END Side Header -->
        <!-- Side Navigation -->
        <div class="content-side content-side-full">
            <ul class="nav-main">
                <li class="nav-main-item">
                    <a class="nav-main-link" href="{{ siteUrl }}components/text">
                        <i class="nav-main-link-icon si si-grid"></i>
                        <span class="nav-main-link-name">RegateText</span>
                    </a>
                </li>
                {#
                <li class="nav-main-item">
                    <a class="nav-main-link" href="/components/text">
                        <i class="nav-main-link-icon si si-grid"></i>
                        <span class="nav-main-link-name">RegateFile</span>
                    </a>
                </li>
                #}
            </ul>
        </div>
        <!-- END Side Navigation -->
      </nav>
      <!-- END Sidebar -->

      <!-- Header -->
      <header id="page-header">
          <!-- Header Content -->
          <div class="content-header">
              <b>{{ title }}</b>
          </div>
          <!-- END Header Content -->
      </header>
      <!-- END Header -->
      <!-- Main Container -->
      <main id="main-container">
        
        {% block body %}{% endblock %}
        
      </main>
      <!-- END Main Container -->
      <!-- Footer -->
      <footer id="page-footer" class="bg-body-light">
          <div class="content py-0">
              <div class="row font-size-sm">
                  <div class="col-sm-6 order-sm-2 mb-1 mb-sm-0 text-center text-sm-right">
                      Created with <i class="fa fa-heart text-danger"></i> by <a class="font-w600" href="https://github.com/dashtinejad" target="_blank">dashtinejad</a>
                  </div>
                  <div class="col-sm-6 order-sm-1 text-center text-sm-left">
                      <a class="font-w600" href="https://github.com/regatejs" target="_blank">Regate</a> &copy; <span data-toggle="year-copy">2018</span>
                  </div>
              </div>
          </div>
      </footer>
      <!-- END Footer -->
  </div>


  <script src="{{ themeUrl }}js/dashmix.app.min.js"></script>

    <script src="{{ themeUrl }}js/plugins/highlightjs/highlight.pack.min.js"></script>

    <script>jQuery(function(){ Dashmix.helpers('highlightjs'); });</script>

  <style>
      pre.sample-code {
          direction: ltr;
          text-align: left;
          color: white;
          background: #6c757d !important;
          padding: 20px;
          margin-top: -20px;
          margin-left: -20px;
          margin-right: -20px;
      }

      pre.sample-code-regate {
      }

      .block-header:hover .permalink-anchor {
          display: inline;
      }

      .block-header .permalink-anchor {
          display: none;
      }

      .regate-example-container{margin:25px 0;background:white}
    .regate-example-container .regate-example{border:0.2rem solid #e6ebf4;padding:20px 20px}
    .regate-example-container .regate-code{background:#f9fafc}
    .regate-example-container .regate-code pre{border-top-left-radius:0;border-top-right-radius:0;margin:0}
    .regate-example-container .regate-code pre code{padding:15px 20px;background:transparent}
    .regate-example-container .regate-terminal{background:#e3e3ea;border-radius:0;padding:15px 20px}
    .regate-example-container .regate-terminal pre{margin:0}
    .regate-example-container .regate-terminal [data-type="string"]{color:maroon}
    .regate-example-container .regate-terminal [data-type="boolean"]{color:purple}
    .regate-example-container .regate-terminal [data-type="number"]{color:blue}
    .regate-example-container .regate-terminal [data-type="title"]{color:gray}
    .regate-example-container .regate-terminal [data-type="value"]{color:black}
    .regate-example-container .regate-terminal code{display:inline-block}
    .regate-example-container .regate-terminal code+code{margin-left:20px}
    .regate-example-container .regate-terminal:before{opacity:0.3;font-family:monospace}
    .regate-example-container .regate-terminal:empty:before{content:'OUTPUT'}
  </style>
</body>
</html>


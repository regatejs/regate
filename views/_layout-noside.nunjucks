<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, shrink-to-fit=no" />
    <title>{{ title }} - Regate</title>

    <script src="{{ themeUrl }}js/core/jquery.min.js"></script>
    <script src="{{ themeUrl }}js/core/bootstrap.bundle.min.js"></script>
    <script src="{{ themeUrl }}js/core/simplebar.min.js"></script>
    <script src="{{ themeUrl }}js/core/jquery-scrollLock.min.js"></script>
    <script src="{{ themeUrl }}js/core/jquery.appear.min.js"></script>
    <script src="{{ themeUrl }}js/core/js.cookie.min.js"></script>

    <link rel="stylesheet" id="css-main" href="{{ themeUrl }}css/dashmix.min.css">
    <link rel="stylesheet" href="{{ themeUrl }}js/plugins/highlightjs/styles/atom-one-light.css">
</head>
<body>

  <div id="page-container" class="_sidebar-o enable-page-overlay side-scroll _page-header-fixed page-header-light main-content-narrow">

      

      <!-- Header -->
      <header id="page-header">
          <!-- Header Content -->
          <div class="content-header">
                <!-- Left Section -->
                <div>
                    <b>{{ title }}</b>
                </div>
                <!-- END Left Section -->
              
                <div>
                    <ul class="nav">
                        {# <li class="nav-item">
                            <a class="nav-link active" href="/">Home</a>
                        </li> #}
                        <li class="nav-item">
                            <a class="nav-link" href="{{ siteUrl }}">Start</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="{{ siteUrl }}components">Components</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="https://github.com/regatejs/regate">GitHub</a>
                        </li>
                    </ul>
                </div>
          </div>
          <!-- END Header Content -->
      </header>
      <!-- END Header -->
      <!-- Main Container -->
      <main id="main-container">
        
        {% block body %}{% endblock %}
        
      </main>
      <!-- END Main Container -->
      <!-- Footer -->
      <footer id="page-footer" class="bg-body-light">
          <div class="content py-0">
              <div class="row font-size-sm">
                  <div class="col-sm-6 order-sm-2 mb-1 mb-sm-0 text-center text-sm-right">
                      Created with <i class="fa fa-heart text-danger"></i> by <a class="font-w600" href="https://github.com/dashtinejad" target="_blank">dashtinejad</a>
                  </div>
                  <div class="col-sm-6 order-sm-1 text-center text-sm-left">
                      <a class="font-w600" href="https://github.com/regatejs" target="_blank">Regate</a> &copy; <span data-toggle="year-copy">2018</span>
                  </div>
              </div>
          </div>
      </footer>
      <!-- END Footer -->
  </div>


  <script src="{{ themeUrl }}js/dashmix.app.min.js"></script>

    <script src="{{ themeUrl }}js/plugins/highlightjs/highlight.pack.min.js"></script>

    <script>jQuery(function(){ Dashmix.helpers('highlightjs'); });</script>

  <style>
      pre.sample-code {
          direction: ltr;
          text-align: left;
          color: white;
          background: #6c757d !important;
          padding: 20px;
          margin-top: -20px;
          margin-left: -20px;
          margin-right: -20px;
      }

      pre.sample-code-regate {
      }

      .block-header:hover .permalink-anchor {
          display: inline;
      }

      .block-header .permalink-anchor {
          display: none;
      }

      .regate-example-container{margin:25px 0;background:white}
    .regate-example-container .regate-example{border:0.2rem solid #e6ebf4;padding:20px 20px}
    .regate-example-container .regate-code{background:#f9fafc}
    .regate-example-container .regate-code pre{border-top-left-radius:0;border-top-right-radius:0;margin:0}
    .regate-example-container .regate-code pre code{padding:15px 20px;background:transparent}
    .regate-example-container .regate-terminal{background:#e3e3ea;border-radius:0;padding:15px 20px}
    .regate-example-container .regate-terminal pre{margin:0}
    .regate-example-container .regate-terminal [data-type="string"]{color:maroon}
    .regate-example-container .regate-terminal [data-type="boolean"]{color:purple}
    .regate-example-container .regate-terminal [data-type="number"]{color:blue}
    .regate-example-container .regate-terminal [data-type="title"]{color:gray}
    .regate-example-container .regate-terminal [data-type="value"]{color:black}
    .regate-example-container .regate-terminal code{display:inline-block}
    .regate-example-container .regate-terminal code+code{margin-left:20px}
    .regate-example-container .regate-terminal:before{opacity:0.3;font-family:monospace}
    .regate-example-container .regate-terminal:empty:before{content:'OUTPUT'}
    .regate-example-container .regate-serialize { font-family:monospace; padding: 5px 20px; font-size:87.5%; }
    .regate-example-container .regate-serialize:empty:before { content: 'SUBMITTED FORM DATA WILL BE SHOWN HERE'; opacity: 0.3;}

    .permalink-anchor { visibility: hidden; }
    h2:hover .permalink-anchor { visibility: visible; }

    .text-xs-center {
        text-align: center;
    }
  </style>

  <script>
    $('.regate-example form').on('submit', function (e) {
        e.preventDefault();
        var $example = $(this).closest('.regate-example-container');
        var $serialize = $example.find('.regate-serialize');
        $serialize.html( $( this ).serialize() );
    });
  </script>
</body>
</html>

